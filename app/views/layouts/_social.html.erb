<% if social_line_enable %>
	<div class="social-line">
		<div class="container">
			<div class="row">
				<div class="col-md-2 col-md-offset-1">
					<%= link_to '#', id: 'vk-link', class: 'btn btn-simple btn-just-icon btn-white' do %>
						<%= fa_icon "vk" %>
					<% end %>
				</div>
				<div class="col-md-2">
					<%= link_to '#', id: 'fb-link', class: 'btn btn-simple btn-just-icon btn-white' do %>
						<%= fa_icon "facebook" %>
					<% end %>
				</div>
				<div class="col-md-2">
					<%= link_to '#', id: 'tw-link', class: 'btn btn-simple btn-just-icon btn-white' do %>
						<%= fa_icon "twitter" %>
					<% end %>
				</div>
				<div class="col-md-2">
					<%= link_to '#', id: 'google-link', class: 'btn btn-simple btn-just-icon btn-white' do %>
						<%= fa_icon "google-plus" %>
					<% end %>
				</div>
				<div class="col-md-2">
					<%= link_to '#', id: 'insta-link', class: 'btn btn-simple btn-just-icon btn-white' do %>
						<%= fa_icon "instagram" %>
					<% end %>
				</div>
			</div>
		</div>
	</div>
<% end %>
